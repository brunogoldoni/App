{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*" : [
        "./src/assets/*"
      ],
      "@components/*" : [
        "./src/components/*"
      ],
      "@screens/*" : [
        "./src/screens/*"
      ],
      "@storage/*" : [
        "./src/storage/*"
      ],
      "@utils/*" : [
        "./src/utils/*"
      ],
       "@theme/*" : [
        "./src/theme/*"
      ],
    }
  }
}
